{
    "namespace": "asa_percent_template",

    "asa_percent_bindings": {
        "type": "label",
        "color": "$percent_color",
        "shadow": "$percent_shadow",
        "$percent_shadow|default": true,
        "localize": false,
        "$asa_binding_condition|default": "none",
        "layer": "$percent_layer",
        "bindings": "$asa_percent_bindings",
        "font_type": "$asa_font",
        "$asa_font|default": "default",
        "font_scale_factor": "$font_scale",
        "$font_scale|default": 1.0,
        "$asa_percent_bindings|default": [
            {
                "binding_name": "$asa_binding_name",
                "binding_condition": "$asa_binding_condition",
                "binding_type": "global"
            },
            {
                "binding_type": "view",
                "source_property_name": "((($asa_binding_name > $min) and ($asa_binding_name < $max)) or ($asa_binding_name = $max))",
                "target_property_name": "#visible"
            }
        ]
    },

    "asa_percent_template": {
        "type": "stack_panel",
        "size": [ "100%c", "100%cm" ],
        "orientation": "horizontal",
        "$reverser_percent_count|default": false,
        "$asa_percent|default": "asa_percent_number",
        "variables": [
            {
                "requires": "$reverser_percent_count",
                "$asa_percent": "asa_percent_number_reverse"
            }
        ],
        "controls": [
            {
                "@$asa_percent": {}
            },
            {
                "@asa_percent_bindings": {
                    "type": "label",
                    "text": "%",
                    "$min": -1,
                    "$max": 1
                }
            }
        ]
    },

