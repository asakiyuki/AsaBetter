{
    "namespace": "asa_search_setting",
    "asa_search_area": {
        "type": "panel",
        "offset": [
            0,
            "21px"
        ],
        "size": [
            "100% - 8px",
            "100% - 19px"
        ],
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "visible": false,
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "89367782_search_setting",
                "source_property_name": "#item_name",
                "target_property_name": "#asa_search"
            },
            {
                "binding_type": "view",
                "source_property_name": "(not (#asa_search = ''))",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "scrolling_panel@common.scrolling_panel": {
                    "size": [
                        "100%",
                        "100%"
                    ],
                    "$scrolling_content": "asa_search_setting.asa_setting_stack",
                    "$show_background": false,
                    "$scroll_size": [
                        "5px",
                        "100% - 4px"
                    ],
                    "$scroll_bar_left_padding_size": [
                        1,
                        0
                    ]
                }
            }
        ]
    },
    "asa_setting_content": {
        "type": "panel",
        "size": [
            "100%",
            "100%c + 2px"
        ],
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "89367782_search_setting",
                "source_property_name": "#item_name",
                "target_property_name": "#asa_search"
            },
            {
                "binding_type": "view",
                "source_property_name": "(not (($setting_name - #asa_search) = $setting_name))",
                "target_property_name": "#visible"
            }
        ]
    },
