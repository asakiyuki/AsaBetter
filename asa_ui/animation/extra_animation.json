{
    "namespace": "asa_extra_animation",

    //Asa RGB
    "asa_rgb_template": {
        "anim_type": "color",
        "duration": "$custom_anim_duration",
        "$custom_anim_duration|default": 0.8
    },

    "rgb_1@asa_rgb_template": {
        "from": [ 0.1, 0.1, 0.7 ],
        "to": [ 0.7, 0.1, 0.7 ],
        "next": "@asa_extra_animation.rgb_2"
    },

    "rgb_2@asa_rgb_template": {
        "from": [ 0.7, 0.1, 0.7 ],
        "to": [ 0.7, 0.1, 0.1 ],
        "next": "@asa_extra_animation.rgb_3"
    },

    "rgb_3@asa_rgb_template": {
        "from": [ 0.7, 0.1, 0.1 ],
        "to": [ 0.7, 0.7, 0.1 ],
        "next": "@asa_extra_animation.rgb_4"
    },

    "rgb_4@asa_rgb_template": {
        "from": [ 0.7, 0.7, 0.1 ],
        "to": [ 0.1, 0.7, 0.1 ],
        "next": "@asa_extra_animation.rgb_5"
    },

    "rgb_5@asa_rgb_template": {
        "from": [ 0.1, 0.7, 0.1 ],
        "to": [ 0.1, 0.7, 0.7 ],
        "next": "@asa_extra_animation.rgb_6"
    },

    "rgb_6@asa_rgb_template": {
        "from": [ 0.1, 0.7, 0.7 ],
        "to": [ 0.1, 0.1, 0.7 ],
        "next": "@asa_extra_animation.rgb_1"
    }
}