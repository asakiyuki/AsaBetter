    //Bossbar Percent
    "asa_bossbar_percent_bindings": {
        "$asa_bossbar_bindings": "#progress_percentage",
        "type": "label",
        "localize": false,
        "layer": 10,
        "shadow": true,
        "color": "$d_color_format",
