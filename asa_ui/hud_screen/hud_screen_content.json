{
    "namespace": "asa_hud_content",

    //Saturation Screen
    "asa_saturation_screen": {
        "type": "panel",
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_saturation_screen",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "@asa_saturation_template": {
                    "$asa_saturation_color": [ 1.0, 0.333, 0.333, 0.2 ],
                    "$asa_binding_tags": "asa_saturation_red"
                }
            },
            {
                "@asa_saturation_template": {
                    "$asa_saturation_color": [ 1.0, 1.0, 0.333, 0.2 ],
                    "$asa_binding_tags": "asa_saturation_yellow"
                }
            },
            {
                "@asa_saturation_template": {
                    "$asa_saturation_color": [ 0.333, 1.0, 0.333, 0.2 ],
                    "$asa_binding_tags": "asa_saturation_green"
                }
            },
            {
                "@asa_saturation_template": {
                    "$asa_saturation_color": [ 1.0, 0.333, 1.0, 0.2 ],
                    "$asa_binding_tags": "asa_saturation_purple"
                }
            },
            {
                "@asa_saturation_template": {
                    "$asa_saturation_color": [ 0.333, 1.0, 1.0, 0.2 ],
                    "$asa_binding_tags": "asa_saturation_aqua"
                }
            },
            {
                "@asa_saturation_template": {
                    "$asa_saturation_color": [ 0.333, 0.333, 1.0, 0.2 ],
                    "$asa_binding_tags": "asa_saturation_blue"
                }
            }
        ]
    },

    "asa_saturation_template": {
        "type": "custom",
        "renderer": "gradient_renderer",
        "layer": -10,
        "color1": "$asa_saturation_color",
        "color2": "$asa_saturation_color",
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "$asa_binding_tags",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            },
            {
                "binding_type": "view",
                "source_control_name": "asa_opacity_saturation",
                "source_property_name": "(#slider_value / 10)",
                "target_property_name": "#alpha"
            }
        ]
    },

    //World Name
    "asa_world_name_bg": {
        "type": "image",
        "size": [ "100%cm + 6px", "100%cm + 4px" ],
        "texture": "textures/ui/hud_tip_text_background",
        "anchor_from": "bottom_right",
        "anchor_to": "bottom_right",
        "offset": [ -1, -1 ],
        "alpha": 0.5,
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_world_name_hud",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "@asa_world_name": {}
            }
        ]
    },

    "asa_world_name": {
        "type": "label",
        "text": "#player_list_title",
        "layer": 5,
        "bindings": [
            {
                "binding_name": "#player_list_title"
            }
        ]
    },

    //Player List HUD
    "asa_player_list_panel": {
        "type": "panel",
        "size": [ "100%cm", "100%cm" ],
        "max_size": [ "100%cm", 166 ],
        "anchor_from": "left_middle",
        "anchor_to": "left_middle",
        "clips_children": true,
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_player_list_hud",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "asa_bg": {
                    "type": "image",
                    "size": [ "100%cm + 2px", "100%cm + 2px" ],
                    "texture": "textures/ui/Black",
                    "anchor_from": "top_middle",
                    "anchor_to": "top_middle",
                    "layer": 50,
                    "offset": [ 0, -0.5 ],
                    "alpha": 0.5,
                    "controls": [
                        {
                            "online_players_grid": {
                                "type": "grid",
                                "size": [ 125, "100%c" ],
                                "grid_item_template": "asa_hud_content.asa_player_name_items",
                                "grid_dimension_binding": "#players_grid_dimension",
                                "collection_name": "players_collection",
                                "bindings": [
                                    {
                                        "binding_name": "#players_grid_dimension",
                                        "binding_condition": "always"
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },

    "asa_player_name_items": {
        "type": "panel",
        "size": [ 125, "100%cm + 1px" ],
        "controls": [
            {
                "asa_stack_hoz": {
                    "type": "stack_panel",
                    "size": [ "100%", 10 ],
                    "orientation": "horizontal",
                    "controls": [
                        {
                            "avatar": {
                                "type": "image",
                                "size": [ 10, 10 ],
                                "texture_file_system": "#texture_source",
                                "bindings": [
                                    {
                                        "binding_name": "#texture",
                                        "binding_type": "collection",
                                        "binding_collection_name": "players_collection"
                                    },
                                    {
                                        "binding_name": "#texture_source",
                                        "binding_name_override": "#texture_file_system",
                                        "binding_type": "collection",
                                        "binding_collection_name": "players_collection"
                                    },
                                    {
                                        "binding_name": "#gamerpic_visible",
                                        "binding_type": "collection",
                                        "binding_collection_name": "players_collection",
                                        "binding_name_override": "#visible"
                                    }
                                ]
                            }
                        },
                        {
                            "empty": {
                                "type": "panel",
                                "size": [ 1, 0 ]
                            }
                        },
                        {
                            "asa_player_gamertag": {
                                "type": "label",
                                "text": "#gamertag",
                                "layer": 2,
                                "size": [ "default", 10 ],
                                "font_scale_factor": 1,
                                "bindings": [
                                    {
                                        "binding_name": "#gamertag",
                                        "binding_type": "collection",
                                        "binding_collection_name": "players_collection"
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },

