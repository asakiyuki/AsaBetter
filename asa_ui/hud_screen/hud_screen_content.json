    "asa_hotbar_button": {
        "type": "button",
        "size": [ 22, 22 ],
        "$asa_hotbar_bindings|default": [
            {
                "binding_name": "#item_id_aux",
                "binding_type": "collection",
                "binding_collection_name": "hotbar_items"
            },
            {
                "binding_type": "view",
                "source_property_name": "(#item_id_aux / #item_id_aux)",
                "target_property_name": "#asa_item_one"
            },
            {
                "binding_type": "view",
                "source_property_name": "(#asa_item_one * 65536)",
                "target_property_name": "#offset_id"
            },
            {
                "binding_type": "view",
                "source_property_name": "(#offset_id * $asa_item_id_offset)",
                "target_property_name": "#asa_offset_2"
            },
            {
                "binding_type": "view",
                "source_property_name": "(#item_id_aux - #asa_offset_2)",
                "target_property_name": "#asa_offset_3"
            },
            {
                "binding_type": "view",
                "source_property_name": "('asa' + #asa_offset_3)",
                "target_property_name": "#asa_item_hold"
            },
            {
                "binding_type": "view",
                "source_property_name": "$asa_source_bar",
                "target_property_name": "#visible"
            }
