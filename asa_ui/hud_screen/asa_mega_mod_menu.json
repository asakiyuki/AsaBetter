            {
                "button_text": {
                    "type": "label",
                    "max_size": [ "100% - 25px", 10 ],
                    "text": "$asa_button_text",
                    "color": "$asa_button_text_color",
                    "font_scale_factor": 0.8,
                    "anchor_from": "left_middle",
                    "anchor_to": "left_middle",
                    "offset": [ 2, 0 ],
                    
                    "locked_alpha": "$asa_locked_alpha",
                    "$asa_locked_alpha|default": 0.25,
