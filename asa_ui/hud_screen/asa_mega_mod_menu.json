                }
            },
            {
                "toggle_tabs": {
                    "type": "image",
                    "size": [ "100%y", "100% - 4px" ],
                    "offset": [ -2, 0 ],
                    "color": "$asa_mod_toggle_color",
                    "anchor_from": "right_middle",
                    "anchor_to": "right_middle",
                    "texture": "asa_textures/ui/button_and_toggle/list_button",
                    
                    "locked_alpha": "$asa_locked_alpha",
