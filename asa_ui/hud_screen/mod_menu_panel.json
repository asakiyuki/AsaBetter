{
    "namespace": "asa_mod_menu_panel",

    "asa_mega_mod_menu_panel": {
        "type": "panel",
        "size": [ "100%", "100%" ],
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "asa_mega_mod_menu",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "stack_ngang": {
                    "type": "stack_panel",
                    "orientation": "horizontal",
                    "size": [ "100%", "100%" ],
                    "controls": [
                        {
                            "mod_menu_panel_1": {
                                "type": "panel",
                                "size": [ "fill", "100%cm" ],
                                "$asa_menu_from": [ "-125%", "-110%" ],
                                "controls": [
                                    {
                                        "@mod_stack_4": {
                                            "animation_reset_name": "asa_button.asa_open_mod_menu",
                                            "anims": [
                                                "@asa_pressed_anims.asa_mod_menu_open_wait"
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "panel": {
                                "type": "panel", "size": [ 1, 1]
                            }
                        },
                        {
                            "mod_menu_panel_1": {
                                "type": "panel",
                                "size": [ "fill", "100%cm" ],
                                "$asa_menu_from": [ "-420", "240%" ],
                                "controls": [
                                    {
                                        "@mod_stack_1": {
                                            "animation_reset_name": "asa_button.asa_open_mod_menu",
                                            "$anims_wait_duration": 0.3,
                                            "anims": [
                                                "@asa_pressed_anims.asa_mod_menu_open_wait"
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "panel": {
                                "type": "panel", "size": [ 1, 1]
                            }
                        },
                        {
                            "mod_menu_panel_1": {
                                "type": "panel",
                                "size": [ "fill", "100%cm" ],
                                "$asa_menu_from": [ -200, "-110%" ],
                                "controls": [
                                    {
                                        "@mod_stack_5": {
                                            "animation_reset_name": "asa_button.asa_open_mod_menu",
                                            "$anims_wait_duration": 0.7,
                                            "anims": [
                                                "@asa_pressed_anims.asa_mod_menu_open_wait"
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "panel": {
                                "type": "panel", "size": [ 1, 1]
                            }
                        },
                        {
                            "mod_menu_panel_1": {
                                "type": "panel",
                                "size": [ "fill", "100%cm" ],
                                "$asa_menu_from": [ "-300%", "500%" ],
                                "controls": [
                                    {
                                        "@mod_stack_3": {
                                            "animation_reset_name": "asa_button.asa_open_mod_menu",
                                            "$anims_wait_duration": 0.9,
                                            "anims": [
                                                "@asa_pressed_anims.asa_mod_menu_open_wait"
                                            ]
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                }
            }
        ]
    },

